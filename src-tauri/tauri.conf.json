{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "PrintNum",
  "version": "0.1.0",
  "identifier": "PrintNum",
  "build": {
    "frontendDist": "../src",
    "removeUnusedCommands": true
  },
  "app": {  
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Печать ячеек",
        "fullscreen": false,
        "resizable": false,
        "maximizable": false,
        "width": 460,
        "height": 275
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": ["icons/tauri_main.ico"],
    "resources": { "../backend/output": "output", "../extensions":"extensions" , "../driver_printer":"driver_printer"}
  }
}
