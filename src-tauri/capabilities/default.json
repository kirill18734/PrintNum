{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:allow-set-size",
    "shell:allow-open",
    "shell:allow-execute",
    {
     "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "run_backend",
          "cmd": "powershell.exe",
          "args": ["../backend/output/backend-x86_64-pc-windows-msvc.exe"]
        }
      ]
    },
     
    "shell:default"
  ]
}